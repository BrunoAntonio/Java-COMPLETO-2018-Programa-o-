package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("How many students for course A?");
		int numbA =sc.nextInt();
		
		for(int i=1; i<=numbA; i++) {
			sc.nextInt();
		}
		
		
		sc.close();
	}

}
