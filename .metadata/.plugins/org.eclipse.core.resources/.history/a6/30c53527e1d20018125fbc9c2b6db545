package application;

import java.util.HashSet;
import java.util.LinkedHashSet;
import java.util.Set;
import java.util.TreeSet;

public class Program {

	public static void main(String[] args) {
		
		//Set<String> set = new HashSet<>(); //mais rápido e não ordenado
		//Set<String> set = new TreeSet<>(); //mais lento e ordenado pelo compareTo do objeto (ou Comparator)
		Set<String> set = new LinkedHashSet<>(); //velocidade intermediária e elementos na ordem em que são adicionados
		
		set.add("Tv");
		set.add("Tablet");
		set.add("Notebook");
		set.add("PS4");
		
		set.remove("Tablet");
		set.removeIf(x -> x.length()>3);
		set.removeIf(x -> x.charAt(0)=='T');
		System.out.println(set.contains("Notebook"));
		System.out.println();
		
		for (String p : set) {
		System.out.println(p);
			
	}
		System.out.println("------------------------------------------------------------------------------------");
}
	
	
	
	
	
}